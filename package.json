    "mime": "^3.0.0"
  },
  "devDependencies": {
    "@types/mime": "^3.0.4",
    "@types/node": "^22.13.5",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.21.0",
